<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lurenman.customcontrolandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CustomTextViewActivity" />
        <activity android:name=".activity.CustomViewGroupActivity" />
        <activity android:name=".activity.ViewGroupExerciseOneActivity" />
        <activity android:name=".activity.ViewGroupExerciseTwoActivity" />
        <activity android:name=".activity.ViewGroupExerciseThreeActivity" />
        <activity android:name=".activity.ViewGroupExerciseFourActivity" />
    </application>

</manifest>